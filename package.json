{
  "name": "md",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:doocs/md.git",
  "author": "jgchenu <jgchenu@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack -p",
    "lint": "yarn eslint && yarn stylelint",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix",
    "eslint": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "eslint:fix": "eslint --fix --ext .ts,.js,.tsx,.jsx src/",
    "stylelint": "stylelint src/**/*.less src/**/*.css",
    "stylelint:fix": "stylelint src/**/*.less src/**/*.css --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "./pre_commit.sh && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint"
    ],
    "*.{css,less}": "stylelint"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@commitlint/config-conventional": "8.3.4",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.4.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "less-loader": "5.0.0",
    "lint-staged": "10.0.7",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "progress-bar-webpack-plugin": "2.1.0",
    "stylelint": "13.2.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-webpack-plugin": "1.2.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.3",
    "antd": "3.26.11",
    "core-js": "3.6.4",
    "less": "3.11.1",
    "react": "16.12.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "4.12.19"
  }
}
